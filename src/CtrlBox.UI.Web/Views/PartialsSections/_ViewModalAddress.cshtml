                    <strong>Reference:</strong>
                    <div class="controls">
                        <span id="reference" class="text">...</span>
                    </div>
                </div>
            </div>
            <div class="span6 ">
                <div class="control-group">
                    <strong>District:</strong>
                    <div class="controls">
                        <span id="district" class="text">...</span>
                    </div>
                </div>
            </div>
        </div>
        <!--/row-->
        <div class="row-fluid">
            <div class="span6 ">
                <div class="control-group">
                    <strong>Post Code:</strong>
                    <div class="controls">
                        <span id="postCode" class="text">457890</span>
                    </div>
                </div>
            </div>
            <!--/span-->
            <div class="span6 ">
                <div class="control-group">
                    <strong>Country:</strong>
                    <div class="controls">
                        <span id="country" class="text">Brasil</span>
                    </div>
                </div>
            </div>
